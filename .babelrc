{
  "presets": [
      "es2017",
      "es2016",
      ["es2015", {"modules": false }],
      "stage-0",
      "react"
  ],
  "plugins": [
      "transform-decorators-legacy",
      "transform-react-jsx",
      ["transform-runtime",
      {
        "helpers": false,
        "polyfill": false,
        "regenerator": true
      }
    ]
  ],
  "comments": false
}